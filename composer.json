{
    "name": "deniztezcan/laravel-postnl-api",
    "description": "Laravel PostNL API package",
    "type": "library",
    "license": "MIT",
    "version": "0.2.1",
    "homepage": "https://github.com/deniztezcan/laravel-postnl-api",
    "authors": [
        {
            "name": "Deniz Tezcan",
            "email": "howdy@deniztezcan.me"
        }
    ],
    "autoload": {
        "psr-4": { "DenizTezcan\\LaravelPostNLAPI\\" : "src/" }
    },
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.*.*|7.*.*|8.*.*"
    },
    "extra": {
      "laravel": {
        "providers": [
          "DenizTezcan\\LaravelPostNLAPI\\PostNLAPIServiceProvider"
        ],
        "aliases": {
          "PostNLAPI": "DenizTezcan\\LaravelPostNLAPI\\Facades\\PostNLAPI"
        }
      }
    }
}
